                         END_CONNECTION_POINT_MAP=*/// \
                         "DECLARE_DYNAMIC(class)= " \
                         "IMPLEMENT_DYNAMIC(class1, class2)= " \
                         "DECLARE_DYNCREATE(class)= " \
                         "IMPLEMENT_DYNCREATE(class1, class2)= " \
                         "IMPLEMENT_SERIAL(class1, class2, class3)= " \
